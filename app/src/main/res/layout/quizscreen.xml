        app:layout_constraintBottom_toBottomOf="@+id/tableRow"
        app:layout_constraintEnd_toStartOf="@+id/checkBox4"
        app:layout_constraintTop_toTopOf="@+id/tableRow" />

    <CheckBox
        android:id="@+id/checkBox4"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="26dp"
        android:layout_marginLeft="25dp"
        android:layout_marginRight="26dp"
        android:layout_marginStart="25dp"
        android:text="Potrait"
        app:layout_constraintBottom_toBottomOf="@+id/checkBox3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/button7"
        app:layout_constraintTop_toTopOf="@+id/checkBox3" />

    <Button
        android:id="@+id/button7"
        android:layout_width="54dp"
        android:layout_height="40dp"
        android:layout_marginBottom="21dp"
        android:layout_marginTop="21dp"
        android:onClick="confirm4"
        android:text="OK"
        app:layout_constraintBottom_toTopOf="@+id/textView19"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/checkBox3" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="0dp"
        android:layout_height="31dp"
        android:layout_marginEnd="12dp"
        android:layout_marginLeft="15dp"
        android:layout_marginRight="12dp"
        android:layout_marginStart="15dp"
        android:text="70+50="
        android:visibility="visible"
        app:layout_constraintBaseline_toBaselineOf="@+id/editText9"
        app:layout_constraintEnd_toStartOf="@+id/editText9"
        app:layout_constraintStart_toStartOf="@+id/tableRow" />

    <EditText
        android:id="@+id/editText9"
        android:layout_width="75dp"
        android:layout_height="0dp"
        android:layout_marginBottom="80dp"
